<div class="row">
    <div class="card">
        <div class="card-header">

            <form class="advance-ticket-book-ui-header search-panel form-horizontal" data-ajax="true" data-ajax-begin="Spiner.show()" data-ajax-complete="Spiner.hide()" data-ajax-method="POST" data-ajax-mode="replace" data-ajax-update="#advance-ticket-book-ui-body" id="SearchScheduleFrom" method="post" novalidate="novalidate" action="/busTicketAdvance/index">

                <div class="validation-summary text-danger validation-summary-valid" data-valmsg-summary="true">
                   <ul>
                       <li style="display:none"></li>
                  </ul>
                </div>

                 <div class="row">
                    <div class="col-md">
                        <div class="form-group has-success">
                            <label class="control-label col-md required" for="RouteId">Route</label>
                            <div class="col-md">
                                <UIHelper:domainSelect custom-attrs="${[onchange: 'LoadSchedules(this.value, "ScheduleId")']}" class="form-control" domain="${Route}"
                                                       name="routeId"  data-val="true" data-val-required="The Route field is required." id="RouteId"/>
                                <span class="text-danger field-validation-valid" data-valmsg-for="RouteId" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="form-group">
                            <label class="control-label col-md" for="Date">Date</label>
                            <div class="col-md">
                                <div class="input-group dp" id="dtp_Date"> <input class="form-control readonly valid" id="Date" name="Date" type="text" value="" aria-invalid="false"> <span class="input-group-addon"><span class="fa fa-calendar"></span></span></div>
                                <span class="text-danger field-validation-valid" data-valmsg-for="Date" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="form-group has-success">
                            <label class="control-label col-md required" for="ScheduleId">Time</label>
                            <div class="col-md">
                                <select class="form-control valid" data-val="true" data-val-required="The Time field is required." id="ScheduleId" name="ScheduleId" aria-describedby="ScheduleId-error" aria-invalid="false"><option value="261377">4:30 AM (101)</option><option value="261742">5:00 AM (102)</option><option value="315772">6:15 AM (105-A/C)</option><option value="316536">6:30 AM (106-M1/AD)</option><option value="316676">6:45 AM (107-A/C)</option><option value="316977">6:55 AM (107-Non/AC)</option><option value="322351">8:00 AM (112)</option><option value="319755">8:30 AM (114)</option><option value="327466">11:00 AM (124)</option><option value="337372">2:30 PM (138)</option><option value="339505">3:00 PM (140)</option><option value="290289">4:00 PM (144)</option><option value="290460">4:30 PM (146)</option><option value="290631">5:00 PM (148)</option><option value="303143">5:30 PM (150)</option><option value="290802">6:00 PM (152)</option><option value="290973">6:30 PM (154)</option><option value="291680">8:00 PM (160)</option><option value="292242">8:30 PM (162)</option><option value="293493">9:30 PM (166)</option><option value="297845" selected="selected">11:30 PM (173-M1/AD)</option><option value="298372">11:50 PM (174-A/C)</option><option value="270590">11:30 PM (175-A/C)</option><option value="270955">11:30 PM (175-Non/AC)</option><option value="331802">5:00 AM (299-SPECIAL)</option><option value="283330">5:30 AM (300-Non/Ac)</option><option value="283685">6:30 AM (301-Tonggi)</option><option value="282975">7:30 AM (302-Tonggi)</option><option value="284395">8:15 AM (303-Tonggi)</option><option value="286285">2:00 PM (308-Non/Ac)</option><option value="287235">4:30 PM (310-Non/Ac)</option><option value="287847">6:00 PM (311-Non/AC)</option><option value="288408">7:30 PM (312-Non/AC)</option><option value="288300">9:00 PM (312-Tonggi)</option><option value="289010">10:00 PM (313-Non/Ac)</option><option value="289554">11:00 AM (315-Tonggi)</option><option value="271615">5:15 AM (601)</option><option value="259116">9:00 PM (610-Non/AC)</option></select>
                                <span class="text-danger field-validation-valid" data-valmsg-for="ScheduleId" data-valmsg-replace="true"></span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md action-button-ticket-search-ui btn-group">
                        <button type="button" id="btnSearchSchedule" class="btn bg-gradient-info btn-sm pull-end" title="Search"><i class="fa fa-search fa-1x"></i></button>
                        <button type="button" class="btn bg-gradient-success btn-sm pull-end" id="btn-refresh-trip"><i class="fa fa-refresh"></i></button>
                    </div>
                </div>
            </form>
        </div>
        <div id="advance-ticket-book-ui-body" class="card-body seat-booking-panel-with-seat-plan-design seat-container advance-ticket-book-ui-body">


        </div>
    </div>
</div>



<script>

    $(document).ready(function () {
        $('#RouteId').trigger('change');
    });

    function LoadSchedules(routeId, loadId) {
        console.log("routeId: "+routeId)
        if (routeId !== "" && routeId !== null) {
            var date = $('#Date').val();
            var schedules = $.ajaxGET('/busTicketAdvance/loadTicketSchedules',
                {
                    routeId: routeId,
                    date: date
                });
            $('#'+loadId).makeDropDown(schedules);
            if (typeof schedules != "undefined" && schedules != null && schedules.length != null && schedules.length > 0) {
                $('#advance-ticket-book-ui-body').empty();
                $('#btnSearchSchedule').trigger('click');
            } else {
                $('#advance-ticket-book-ui-body').empty();
                $('#'+loadId).makeDropDown(schedules, "--Select--");
                var compiled = '<div class="empty-state">\n' +
                    '    <div class="empty-state__content">\n' +
                    '        <div class="empty-state__icon">\n' +
                    '            <img src="data:image/png;base64," alt="">\n' +
                    '        </div>\n' +
                    '        <div class="empty-state__message">No Data Found!!!</div>\n' +
                    '    </div>\n' +
                    '</div>'

                $("#advance-ticket-book-ui-body").html(compiled);
            }

        }
    }

    $("#dtp_Date").datetimepicker(
        {
            format: 'MMM DD, YYYY',
            minDate: moment().millisecond(0).second(0).minute(0).hour(0),
            defaultDate: moment()
        }).on("dp.hide", function (e) {

        LoadSchedules($('#RouteId').val(), 'ScheduleId');
    });


    $('#btnSearchSchedule').on('click',function(e) {
        if ($('#ScheduleId').val() === null || $('#ScheduleId').val() === '') {
            var compiled = '<div class="empty-state">\n' +
                '    <div class="empty-state__content">\n' +
                '        <div class="empty-state__icon">\n' +
                '            <img src="data:image/png;base64," alt="">\n' +
                '        </div>\n' +
                '        <div class="empty-state__message">No Data Found!!!</div>\n' +
                '    </div>\n' +
                '</div>'
            $("#advance-ticket-book-ui-body").html(compiled);
            e.preventDefault();
        } else {
            $.ajax({
                url: 'busTicketAdvance/bookingPanel',
                method: 'GET',
                dataType: 'html',
                async: false,
                data:{id:  $('#ScheduleId').val(), date: $("#dtp_Date [name=Date]").val()},
                success: function (data) {
                  var bookingPanel = data;
                  $("#advance-ticket-book-ui-body").html(bookingPanel);
                }
            });
        }

    });

    $('#ScheduleId').on('change',function() {
        if ($('#ScheduleId').val() !== null || $('#ScheduleId').val() !== '')
            $('#btnSearchSchedule').trigger('click');
    });

</script>
